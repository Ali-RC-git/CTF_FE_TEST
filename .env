# CTF Backend API Configuration
# Copy this file to .env.local and update the values as needed
# 
# To use this configuration:
# 1. Copy this file: cp env.template .env.local
# 2. Update the values below as needed
# 3. Restart your development server

# API Base URL - Change this to your backend server URL
NEXT_PUBLIC_API_BASE_URL=http://ctf-backend-unique.eastus.azurecontainer.io:8000/api/v1

# API Timeout in milliseconds (default: 10000)
NEXT_PUBLIC_API_TIMEOUT=10000

# Enable API request/response logging (default: false)
# Set to 'true' to see API requests/responses in browser console
NEXT_PUBLIC_API_LOGGING=true

# Environment (development, production, test)
NODE_ENV=development
